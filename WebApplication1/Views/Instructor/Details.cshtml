@model Instructor
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>


<div id="details" class="card" style="width:400px">
    <img class="card-img-top" src="~/Images/@Model.imageUrl" alt="Card image">
    <div class="card-body">
        <h4 class="card-title">@Model.Name</h4>
        <p class="card-text">@Model.salary</p>
        <p class="card-text">@Model.address</p>
        <p class="card-text">@Model.address</p>
        <p class="card-text"><a href="/Instructor/All">all instructors</a></p>
    </div>
</div>
